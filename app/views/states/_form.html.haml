= form_for(@state) do |f|
- if @state.errors.any?
#error_explanation
  %h2
    = pluralize(@state.errors.count, "error")
    prohibited this state from being saved:
  %ul
    - @state.errors.full_messages.each do |msg|
    %li
      = msg
    - end
- end
.field
  = f.label :symbol
  %br
  = f.text_field :symbol
.field
  = f.label :name
  %br
  = f.text_field :name
.actions
  = f.submit
- end
